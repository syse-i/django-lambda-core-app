msgid "Please contact us if you have any trouble resetting your password."
msgstr "Póngase en contacto con nosotros si tiene algún problema para restablecer su contraseña."

#: lambda_core/templates/lambda_theme/tailwindcss/account/password_reset_done.html:21
msgid "We have sent you an e-mail. Please contact us if you do not receive it within a few minutes."
msgstr "Le hemos enviado un e-mail. Póngase en contacto con nosotros si no lo recibe en unos minutos."

#: lambda_core/templates/lambda_theme/tailwindcss/account/password_reset_from_key.html:13
msgid "Bad Token"
msgstr "Mal token"

#: lambda_core/templates/lambda_theme/tailwindcss/account/password_reset_from_key.html:21
#, python-format
msgid "The password reset link was invalid, possibly because it has already been used. Please request a <a href=\"%(password_reset_url)s\">new password reset</a>."
msgstr "El enlace de restablecimiento de contraseña no era válido, posiblemente porque ya se ha utilizado. Solicite un <a href=\"%(password_reset_url)s\">nuevo restablecimiento</a>de contraseña ."

